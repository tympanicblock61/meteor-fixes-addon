{
  "schemaVersion": 1,
  "id": "fixes-addon",
  "version": "${version}",
  "name": "Meteor Fixes Addon",
  "description": "A addon for meteor client that adds fixes and stuff that should be added",
  "authors": [
    "tympanic"
  ],
  "contact": {
    "repo": "https://github.com/tympanicblock61/meteor-fixes-addon"
  },
  "icon": "assets/fixes/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.tympanic.fixes.Addon"
    ]
  },
  "mixins": [
    "Fixes-Addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
